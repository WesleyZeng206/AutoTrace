# Database
DATABASE_URL=postgresql://user:password@localhost:5432/autotrace

# Server
PORT=4000
NODE_ENV=development

# CORS 
ALLOWED_ORIGINS=http://localhost:3000

# Rate Limiting 
RATE_LIMIT_REQUESTS=100   
TELEMETRY_RATE_LIMIT=500     

# Trust Proxy Headers
TRUST_PROXY=0

# Authentication
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
SESSION_SECRET=your-random-32-character-secret-here
BCRYPT_ROUNDS=10           
SESSION_DURATION_DAYS=7

# Optional 
API_KEY= your-api-key

# Aggregator 
AGGREGATOR_ENABLED=true

# Options: 30m, 1h, 2h, etc.
AGGREGATOR_INTERVAL=1h         
AGGREGATOR_RUN_ON_STARTUP=false
